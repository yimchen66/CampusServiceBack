<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.RecommandOrderMapper">

    <resultMap id="BaseResultMap" type="com.example.model.domain.RecommandOrder">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="orderTitle" column="order_title" jdbcType="VARCHAR"/>
            <result property="orderPicture" column="order_picture" jdbcType="VARCHAR"/>
            <result property="orderHot" column="order_hot" jdbcType="INTEGER"/>
            <result property="orderLabel" column="order_label" jdbcType="VARCHAR"/>
            <result property="hostName" column="host_name" jdbcType="VARCHAR"/>
            <result property="hostFigureUrl" column="host_figure_url" jdbcType="VARCHAR"/>
            <result property="money" column="money" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,order_title,order_picture,
        order_hot,order_label,host_name,
        host_figure_url,money
    </sql>
</mapper>
