<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.TransHistoryMapper">

    <resultMap id="BaseResultMap" type="com.example.model.domain.TransHistory">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="orderId" column="order_id" jdbcType="VARCHAR"/>
            <result property="finishTime" column="finish_time" jdbcType="TIMESTAMP"/>
            <result property="orderMoney" column="order_money" jdbcType="DOUBLE"/>
            <result property="hostId" column="host_id" jdbcType="VARCHAR"/>
            <result property="acceptId" column="accept_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,order_id,finish_time,
        order_money,host_id,accept_id
    </sql>
</mapper>
